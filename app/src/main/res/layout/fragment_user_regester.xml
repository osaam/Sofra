<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_login_restorant"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background_login_res_color"
        />
    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/fragment_regester_user_img"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"
        android:layout_marginTop="40dp"
        android:background="@drawable/ic_account_white"
        />

    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_below="@+id/fragment_regester_user_img"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_account_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_login_user_et_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/name"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_login_user_et_name"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_regester_user_relative_name"
        android:layout_marginTop="10dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_email_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_regester_user_et_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/email"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_regester_user_et_email"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_regester_user_relative_email"
        android:layout_marginTop="10dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_phone_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_regester_user_et_phone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/phone"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_regester_user_et_phone"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_city"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_regester_user_relative_phone"
        android:layout_marginTop="10dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_home_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_regester_user_et_city"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="60dp"
            android:hint="@string/city"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <Spinner
            android:id="@+id/fragment_regester_user_sp_city"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@null"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_regester_user_et_city"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arrow_down_white"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_regon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_regester_user_relative_city"
        android:layout_marginTop="10dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_home_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_regester_user_et_regon"
            android:layout_width="@dimen/_300sdp"
            android:layout_height="60dp"
            android:hint="@string/area"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <Spinner
            android:id="@+id/fragment_regester_user_sp_regon"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@null"

            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_regester_user_et_regon"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_arrow_down_white"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            />
    </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fragment_regester_user_relative_pass"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/fragment_regester_user_relative_regon"
            android:layout_marginTop="10dp"
            android:layout_marginStart="25dp"
            android:layout_marginEnd="25dp"
            >
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:background="@drawable/ic_lock_white"
                android:layout_centerVertical="true"
                android:layout_marginStart="10dp"/>

            <EditText
                android:id="@+id/fragment_regester_user_et_pass"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/password"
                android:layout_marginStart="40dp"
                android:textColorHint="@color/white"
                android:inputType="textPassword"
                android:textSize="20dp"
                />
            <ImageView
                android:layout_width="320dp"
                android:layout_height="1dp"
                android:layout_alignBottom="@id/fragment_regester_user_et_pass"
                android:background="@color/white"
                android:layout_marginStart="40dp"
                android:layout_marginBottom="8dp"
                />

        </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_regester_user_relative_pass_confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fragment_regester_user_relative_pass"
        android:layout_marginTop="10dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_lock_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_regester_user_et_pass_confirm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/password_confirmation"
            android:inputType="textPassword"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_regester_user_et_pass_confirm"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>



    <Button
        android:id="@+id/fragment_regester_user_btn_confirm"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_below="@+id/fragment_regester_user_relative_pass_confirm"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="50dp"
        android:background="@drawable/btn_grident"
        android:text="@string/confirm"
        android:textColor="@color/white"
        android:textSize="20sp"/>


</RelativeLayout>
