<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"

    >

<ImageView
        android:id="@+id/fragment_restorant_modefy_img"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerHorizontal="true"
        android:textColor="@color/white"
        android:layout_marginTop="10dp"
        android:layout_margin="30dp"
        android:background="@drawable/ic_account_gray"
        />

    <RelativeLayout
        android:id="@+id/fragment_restorant_modefy_relative_name"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="@drawable/circle_gray"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_below="@+id/fragment_restorant_modefy_img"

        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_account_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_login_restorant_txt_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/name_of_resturante"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_login_restorant_txt_name"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_restorant_modefy_relative_email"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="@drawable/circle_gray"
        android:layout_below="@+id/fragment_restorant_modefy_relative_name"
        android:layout_marginTop="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        >
        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_email_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp"/>

        <EditText
            android:id="@+id/fragment_restorant_modefy_et_email"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/email"
            android:layout_marginStart="40dp"
            android:textColorHint="@color/white"
            android:textSize="20dp"
            />
        <ImageView
            android:layout_width="320dp"
            android:layout_height="1dp"
            android:layout_alignBottom="@id/fragment_restorant_modefy_et_email"
            android:background="@color/white"
            android:layout_marginStart="40dp"
            android:layout_marginBottom="8dp"
            />

    </RelativeLayout>
