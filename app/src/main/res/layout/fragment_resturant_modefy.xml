    <RelativeLayout
        android:id="@+id/fragment_restorant_modefy_relative_delevary"
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:background="@drawable/circle_gray"
        android:layout_below="@+id/fragment_restorant_modefy_relative_regon"
        android:layout_marginTop="20dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@drawable/ic_money_white"
            android:layout_centerVertical="true"
            android:layout_marginStart="10dp" />

        <EditText
            android:id="@+id/fragment_restorant_modefy_et_delevary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="@string/delevary_fee"
